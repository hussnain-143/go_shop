{
  "swagger": "2.0",
  "info": {
    "description": "\n\nHeader -> api_key :  \n\n",
    "version": "1.0.0",
    "title": "Swagger App"
  },
  "host": "127.0.0.1:8000",
  "basePath": "/api",
  "tags": [
    {
      "name": "User",
      "description": "All User Operations"
