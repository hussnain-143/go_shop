        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "description": "Enter Bearer Token",
            "required": true,
            "type": "string"
          },
          {
            "name": "name",
            "in": "formData",
            "description": "Enter Name",
            "required": true,
            "type": "string"
